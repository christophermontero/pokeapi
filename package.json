{
  "name": "rocketmon-api",
  "version": "1.0.0",
  "description": "This is an API for visualize all pokemons with their stats",
  "main": "src/index.ts",
  "scripts": {
    "start": "pm2 start ecosystem.config.js --no-daemon",
    "build": "tsc",
    "dev": "cross-env NODE_ENV=development nodemon src/index.ts",
    "test": "NODE_ENV=test jest --detectOpenHandles",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christophermontero/rocketmon-api.git"
  },
  "keywords": [
    "api",
    "Pokemon",
    "node",
    "node.js",
    "express",
    "rest",
    "mongodb",
    "mongoose",
    "es6",
    "es7",
    "es8",
    "es9",
    "jest",
    "joi",
    "eslint",
    "prettier"
  ],
  "author": "giocom",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/christophermontero/rocketmon-api/issues"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "homepage": "https://github.com/christophermontero/rocketmon-api#readme",
  "dependencies": {
    "bcrypt": "~5.1.0",
    "colors": "~1.4.0",
    "compression": "~1.7.4",
    "cors": "~2.8.5",
    "cross-env": "~7.0.3",
    "dotenv": "~16.3.1",
    "express": "~4.18.2",
    "express-mongo-sanitize": "~2.2.0",
    "express-rate-limit": "~7.1.1",
    "helmet": "~6.0.1",
    "http-status": "~1.7.3",
    "joi": "~17.9.1",
    "jsonwebtoken": "~9.0.0",
    "lodash": "~4.17.21",
    "mongoose": "~7.0.3",
    "morgan": "~1.10.0",
    "pm2": "~5.3.0",
    "swagger-ui-express": "~4.6.2",
    "winston": "~3.11.0"
  },
  "devDependencies": {
    "@faker-js/faker": "~8.2.0",
    "@types/bcrypt": "~5.0.0",
    "@types/compression": "~1.7.4",
    "@types/cors": "~2.8.15",
    "@types/express": "~4.17.20",
    "@types/jest": "~29.5.0",
    "@types/jsonwebtoken": "~9.0.1",
    "@types/lodash": "~4.14.192",
    "@types/morgan": "~1.9.4",
    "@types/supertest": "~2.0.12",
    "@types/swagger-ui-express": "~4.1.3",
    "@types/winston": "~2.4.4",
    "@typescript-eslint/eslint-plugin": "~6.9.0",
    "@typescript-eslint/parser": "~6.9.0",
    "eslint": "~8.52.0",
    "eslint-config-prettier": "~9.0.0",
    "eslint-plugin-prettier": "~5.0.1",
    "husky": "~8.0.3",
    "jest": "~29.5.0",
    "lint-staged": "~15.0.2",
    "node-mocks-http": "~1.13.0",
    "nodemon": "~2.0.22",
    "prettier": "~3.0.3",
    "supertest": "~6.3.3",
    "ts-jest": "~29.1.0",
    "ts-node": "~10.9.1",
    "typescript": "~5.0.4"
  }
}
